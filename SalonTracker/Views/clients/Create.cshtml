@model SalonTracker.Models.clients
@using SalonTracker.Models;

<h3>Add a new Client</h3>
@using (Html.BeginForm())
{
  <label for="clientName">Clients Name: </label>
  @Html.TextBoxFor(model => model.clientName)
  <label for="stylistId">Stylist: </label>
  @Html.DropDownList("stylistId")
  <input type="submit" value="Add new Client">
}